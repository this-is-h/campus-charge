{
    "functions": {
        "api/php/*.php": {
            "runtime": "vercel-php@0.6.0",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "redirects": [
        { "source": "/charge/:match*", "destination": "/:match*" },
        { "source": "/api/:match*", "destination": "/api/php/:match*" },
        { "source": "/[^\/]*", "destination": "/403", "statusCode": 403 }
    ]
}